@media only screen and (min-width: 1001px) {
    .timetable-cell-flight-status {
        display: table-cell;
        text-align: center;
        vertical-align: middle;
        width: 12em;
        background-clip: padding-box;

        -o-transition: .135s;
        -ms-transition: .135s;
        -moz-transition: .135s;
        -webkit-transition: .135s;
        transition: .135s;

        &._hover {
            outline: 0;
            background: rgba(0, 255, 0, 0.2) none repeat scroll 0 0;
        }
    }
}
